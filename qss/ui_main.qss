QListWidget {
    outline: 0px;
}

QListWidget#CommandList {
    min-width: 120px;
    max-width: 120px;
    color: Black;
    background: rgb(51, 51, 51);
    font-size: 28px;
}

QListWidget#CommandList::Item:selected {
    background: rgb(88, 88, 88);
    border-left: 5px solid red;
}

QListWidget#CommandList::Item:hover {
    background: rgb(95, 95, 95);
}


QListWidget#DownloadTypeList {
    min-width: 160px;
    max-width: 160px;
    color: Black;
    background: rgb(244, 245, 247);
    font-size: 28px;
}

QListWidget#DownloadTypeList::Item:selected {
    background: lightblue;
    border-left: 5px solid red;
}

QListWidget#DownloadTypeList::Item:hover {
    background: rgb(176, 224, 230);
}

QPushButton#SelectBtFile {
    border: 2px dashed;
    min-width: 80px;
    border-color: lightgray;
    outline: none;
}

QPushButton#SelectBtFile:focus {
    border-color: lightblue;
}

QPushButton#SelectBtFile:pressed {
    background-color: lightblue;
    padding-left:6px;
    padding-top:6px;
}

QPushButton#SelectBtFile:hover {
    border-color: lightblue;
}

QPushButton#BlockItem {
    border: none;
    min-width: 10px;
    min-height: 10px;
    max-width: 10px;
    max-height: 10px;
    background: green;
    outline: none;
}

QPushButton#BlockItem:!enabled {
    border: 1px solid lightgray;
    min-width: 10px;
    min-height: 10px;
    max-width: 10px;
    max-height: 10px;
    background: white;
    outline: none;
}

QPushButton#ButtonOpenFile {
    max-width: 24px;
    min-width: 24px;
    min-height: 24px;
    max-height: 24px;
    border-radius: 13px;
    border:1px solid #9f9f9f;
	image:url(icons/open.png);
    padding: 2px;
}

QPushButton#ButtonOpenFile:focus {
