QTabBar#NewTaskTab::tab {
    min-height: 42px;
    min-width: 160px;
    font-size: 24px;
    border: None;
}

QTabBar#NewTaskTab::tab:hover {
    border-bottom: 5px solid #C2C7CB;
}

QTabBar#NewTaskTab::tab:selected {
    background: lightblue;
    border-top: None;
    border-bottom: 5px solid red;
}

QTabBar#TaskDetailsTab::tab {
    min-height: 42px;
    min-width: 140px;
    font-size: 18px;
    border: None;
    border-bottom: 2px solid #EFEFEF;
}

QTabBar#TaskDetailsTab::tab:hover {
    background: lightblue;
    border-bottom: 2px solid #C2C7CB;
}

QTabBar#TaskDetailsTab::tab:selected {
    background: lightblue;
    border-top: None;
    border-bottom: 2px solid red;
}