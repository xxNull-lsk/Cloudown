QListWidget, QListView, QTreeWidget, QTreeView {
    outline: 0px;
}

QListWidget {
    min-width: 160px;
    max-width: 160px;
    color: Black;
    background: #F5F5F5;
    font-size: 28px;
}

QListWidget::Item:selected {
    background: lightblue;
    border-left: 5px solid red;
}
HistoryPanel:hover {
    background: rgb(52, 52, 52);
}