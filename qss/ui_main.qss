QListWidget, QListView, QTreeWidget, QTreeView {
    outline: 0px;
}

QListWidget {
    min-width: 120px;
    max-width: 120px;
    color: Black;
    background: rgb(51, 51, 51);
    font-size: 28px;
}

QListWidget::Item:selected {
    background: rgb(88, 88, 88);
    border-left: 5px solid red;
}
HistoryPanel:hover {
    background: rgb(52, 52, 52);
}