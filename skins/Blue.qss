QTabBar#NewTaskTab::tab {
    min-height: 42px;
    min-width: 160px;
    font-size: 24px;
    border: None;
}

QTabBar#NewTaskTab::tab:hover {
    border-bottom: 5px solid rgb(70, 172, 232);
}

QTabBar#NewTaskTab::tab:selected {
    background: rgb(70, 172, 232);
    border-top: None;
    border-bottom: 5px solid rgb(255, 128, 0);
}

QTabBar#TaskDetailsTab::tab {
    min-height: 42px;
    min-width: 140px;
    font-size: 18px;
    border: None;
    border-bottom: 2px solid rgb(232, 243, 247);
}

QTabBar#TaskDetailsTab::tab:hover {
    background: rgb(70, 172, 232);
    border-bottom: 2px solid rgb(70, 172, 232);
}

QTabBar#TaskDetailsTab::tab:selected {
    background: rgb(70, 172, 232);
    border-top: None;
    border-bottom: 2px solid rgb(255, 128, 0);
}